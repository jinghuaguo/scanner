<conf>

  <output>
    <output_directory>/tmp/o</output_directory>
    <object_name>fu</object_name>
    <save_intermediate_results>0</save_intermediate_results>
  </output>

  <camera>
    <width>960</width>
    <height>720</height>
    <Logitech_Quickcam_9000_raw_mode>0</Logitech_Quickcam_9000_raw_mode>
  </camera>

  <projector>
    <width>1024</width>
    <height>768</height>
    <invert_projector>0</invert_projector>
  </projector>

  <gain>
    <camera_gain>50</camera_gain>
    <projector_gain>50</projector_gain>
  </gain>

  <distortion_model>
    <enable_tangential_camera>0</enable_tangential_camera>
    <enable_6th_order_radial_camera>0</enable_6th_order_radial_camera>
    <enable_tangential_projector>0</enable_tangential_projector>
    <enable_6th_order_radial_projector>0</enable_6th_order_radial_projector>
  </distortion_model>

  <camera_chessboard>
    <interior_horizontal_corners>8</interior_horizontal_corners>
    <interior_vertical_corners>6</interior_vertical_corners>
    <square_width_mm>30.0</square_width_mm>
    <square_height_mm>30.0</square_height_mm>
  </camera_chessboard>

  <projector_chessboard>
    <interior_horizontal_corners>8</interior_horizontal_corners>
    <interior_vertical_corners>6</interior_vertical_corners>
    <square_width_pixels>75</square_width_pixels>
    <square_height_pixels>75</square_height_pixels>
  </projector_chessboard>

  <scanning_and_reconstruction>	
    <mode>2</mode>
    <reconstruct_columns>1</reconstruct_columns>
    <reconstruct_rows>1</reconstruct_rows>
    <frame_delay_ms>200</frame_delay_ms>
    <minimum_contrast_threshold>32</minimum_contrast_threshold>
    <minimum_distance_mm>0.0</minimum_distance_mm>
    <maximum_distance_mm>10000</maximum_distance_mm>
    <maximum_distance_variation_mm>10.0</maximum_distance_variation_mm>
    <minimum_background_distance_mm>20.0</minimum_background_distance_mm>
  </scanning_and_reconstruction>	

  <visualization>
    <display_intermediate_results>1</display_intermediate_results>
    <display_window_width_pixels>640</display_window_width_pixels>
  </visualization>

</conf>
